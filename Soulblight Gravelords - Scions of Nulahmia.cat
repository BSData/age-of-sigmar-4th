            <selectionEntry type="upgrade" import="true" name="Amulet of Leeches" hidden="false" id="2cf1-187d-f040-7dd4">
              <profiles>
                <profile name="Amulet of Leeches" typeId="907f-a48-6a04-f788" typeName="Ability (Passive)" hidden="false" id="d0d0-d768-3927-c6d6">
                  <characteristics>
                    <characteristic name="Keywords" id="858a-8321-23b2-7117" hidden="false" typeId="b977-7c5e-33b2-428e"/>
