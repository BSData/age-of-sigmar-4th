          <characteristics>
            <characteristic name="Keywords" typeId="b977-7c5e-33b2-428e"/>
            <characteristic name="Effect" typeId="fd7f-888d-3257-a12b">Ignore the first damage point that would be allocated to each friendly **^^Petrifex Elite^^** unit in each phase.</characteristic>
          </characteristics>
