          <selectionEntries>
            <selectionEntry type="upgrade" import="true" name="Slippery Skumbag" hidden="false" id="600f-eb3b-4263-d4">
              <profiles>
                <profile name="Slippery Skumbag" typeId="59b6-d47a-a68a-5dcc" typeName="Ability (Activated)" hidden="false" id="afbd-a16a-b92c-e487">
                  <characteristics>
                    <characteristic name="Timing" typeId="652c-3d84-4e7-14f4">Any Combat Phase</characteristic>
                    <characteristic name="Declare" typeId="bad3-f9c5-ba46-18cb"/>
                    <characteristic name="Effect" typeId="b6f1-ba36-6cd-3b03">If this unit is in combat, roll a dice. On a 3+, it can immediately use the ‘Retreat’ ability as if it were your movement phase without any mortal damage being inflicted on it.</characteristic>
                    <characteristic name="Keywords" typeId="12e8-3214-7d8f-1d0f"/>
                  </characteristics>
                </profile>
              </profiles>
              <constraints>
