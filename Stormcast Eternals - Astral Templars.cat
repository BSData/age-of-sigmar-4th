          <characteristics>
            <characteristic name="Timing" typeId="736-6e3a-d0b5-a1b0">Your Movement Phase</characteristic>
            <characteristic name="Cost" typeId="a49e-3082-e2a6-e802">1</characteristic>
            <characteristic name="Declare" typeId="b77f-7548-840e-c086">Pick a friendly **^^Astral Templars^^** unit to be the target.</characteristic>
            <characteristic name="Effect" typeId="2111-3ca8-61dd-a5f0">The next time you make a run roll for the target this phase, roll 1 additional dice, to a maximum of 2.</characteristic>
            <characteristic name="Keywords" typeId="445d-f443-5448-e7ce"/>
